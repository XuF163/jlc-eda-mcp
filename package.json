{
	"name": "jlceda-mcp",
	"private": false,
	"version": "0.0.13",
	"repository": {
		"type": "git",
		"url": "https://github.com/XuF163/jlc-eda-mcp"
	},
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"prebuild": "node ./scripts/sync-versions.mjs",
		"version:check": "node ./scripts/check-versions.mjs",
		"version:sync": "node ./scripts/sync-versions.mjs",
		"build:mcp": "npm -w packages/mcp-server run build",
		"build:ext": "npm -w packages/eda-extension run build",
		"build": "npm run build:mcp && npm run build:ext",
		"typecheck:mcp": "npm -w packages/mcp-server run typecheck",
		"typecheck:ext": "npm -w packages/eda-extension run typecheck",
		"typecheck": "npm run typecheck:mcp && npm run typecheck:ext"
	},
	"engines": {
		"node": ">=20.5.0"
	}
}
